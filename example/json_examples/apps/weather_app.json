                            "mainAxisAlignment": "start",
                            "crossAxisAlignment": "center",
                            "mainAxisSize": "max"
                          },
                          {
                            "type": "column",
                            "children": [
                              {
                                "type": "icon",
                                "icon": "water_drop",
                                "size": 24.0,
                                "color": "#4CAF50"
                              },
                              {
                                "type": "text",
                                "value": "Humidity"
                              },
                              {
                                "type": "text",
                                "value": "{{currentWeather.humidity}}%",
