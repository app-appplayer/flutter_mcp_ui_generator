              {
                "type": "sizedbox",
                "height": 12.0
              },
              {
                "type": "container",
                "child": {
                  "type": "listview",
                  "items": "{{hourlyForecast}}",
                  "itemTemplate": {
                    "type": "card",
                    "child": {
                      "type": "container",
                      "child": {
                        "type": "column",
                        "children": [
                          {
                            "type": "text",
                            "value": "{{item.time}}",
                            "style": {
                              "fontSize": 12.0
                            }
                          },
                          {
                            "type": "icon",
                            "icon": "{{item.icon}}",
                            "size": 24.0,
                            "color": "#FF9800"
                          },
                          {
                            "type": "text",
                            "value": "{{item.temp}}째",
                            "style": {
                              "fontWeight": "bold"
                            }
                          }
                        ],
                        "mainAxisAlignment": "spaceAround",
                        "crossAxisAlignment": "center",
                        "mainAxisSize": "max"
                      },
                      "width": 80.0,
                      "padding": {
                        "all": 12.0
                      }
                    },
                    "elevation": 2.0
                  },
                  "itemSpacing": 12.0,
                  "shrinkWrap": false,
                  "scrollDirection": "horizontal"
                },
                "height": 120.0
              },
              {
                "type": "sizedbox",
                "height": 20.0
              },
              {
                "type": "text",
                "value": "7-Day Forecast",
                "style": {
                  "fontSize": 18.0,
                  "fontWeight": "bold"
                }
              },
              {
                "type": "sizedbox",
                "height": 12.0
              },
              {
                "type": "expanded",
                "child": {
                  "type": "listview",
                  "items": "{{weeklyForecast}}",
                  "itemTemplate": {
                    "type": "card",
                    "child": {
                      "type": "listtile",
                      "title": "{{item.day}}",
                      "subtitle": "{{item.condition}}",
                      "leading": {
                        "type": "icon",
                        "icon": "{{item.icon}}",
                        "size": 32.0,
                        "color": "#FF9800"
                      },
                      "trailing": {
                        "type": "row",
                        "children": [
                          {
                            "type": "text",
                            "value": "{{item.high}}째",
                            "style": {
                              "fontWeight": "bold"
                            }
                          },
                          {
                            "type": "text",
                            "value": "/"
                          },
                          {
                            "type": "text",
                            "value": "{{item.low}}째",
                            "style": {
                              "color": "#666666"
                            }
                          }
                        ],
                        "mainAxisAlignment": "start",
                        "crossAxisAlignment": "center",
                        "mainAxisSize": "min"
                      }
                    },
                    "elevation": 2.0
                  },
                  "itemSpacing": 4.0,
                  "shrinkWrap": false,
                  "scrollDirection": "vertical"
                },
                "flex": 1
              }
            ],
            "mainAxisAlignment": "start",
            "crossAxisAlignment": "center",
            "mainAxisSize": "max"
          },
          "padding": {
            "all": 16.0
          }
        },
        "flex": 1
      }
