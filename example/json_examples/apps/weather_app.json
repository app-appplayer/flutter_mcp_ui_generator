{
  "type": "page",
  "title": "Weather App",
  "content": {
    "type": "column",
    "children": [
      {
        "type": "appbar",
        "title": "Weather",
        "actions": [
          {
            "type": "button",
            "label": "",
            "onTap": {
              "type": "tool",
              "tool": "refreshWeather"
            },
            "style": "text",
            "icon": "refresh",
            "disabled": false
          }
        ],
        "elevation": 4.0
      },
      {
        "type": "expanded",
        "child": {
          "type": "padding",
          "child": {
            "type": "column",
            "children": [
              {
                "type": "card",
                "child": {
                  "type": "padding",
                  "child": {
                    "type": "column",
                    "children": [
                      {
                        "type": "text",
                        "value": "{{currentWeather.location}}",
                        "style": {
                          "fontSize": 24.0,
                          "fontWeight": "bold"
                        }
                      },
                      {
                        "type": "sizedbox",
                        "height": 8.0
                      },
                      {
                        "type": "text",
                        "value": "{{currentWeather.date}}",
                        "style": {
                          "fontSize": 16.0,
                          "color": "#666666"
                        }
                      },
                      {
                        "type": "sizedbox",
                        "height": 24.0
                      },
                      {
                        "type": "row",
                        "children": [
                          {
                            "type": "icon",
                            "icon": "wb_sunny",
                            "size": 64.0,
                            "color": "#FF9800"
                          },
                          {
                            "type": "sizedbox",
                            "width": 24.0
                          },
                          {
                            "type": "column",
                            "children": [
                              {
                                "type": "text",
                                "value": "{{currentWeather.temperature}}째",
