                              {
                                "type": "text",
                                "value": "{{currentWeather.condition}}",
                                "style": {
                                  "fontSize": 18.0
                                }
                              }
                            ],
                            "mainAxisAlignment": "start",
                            "crossAxisAlignment": "center",
                            "mainAxisSize": "max"
                          }
                        ],
                        "mainAxisAlignment": "center",
                        "crossAxisAlignment": "center",
                        "mainAxisSize": "max"
                      },
                      {
                        "type": "sizedbox",
                        "height": 24.0
                      },
                      {
                        "type": "row",
                        "children": [
                          {
                            "type": "column",
                            "children": [
                              {
                                "type": "icon",
                                "icon": "air",
                                "size": 24.0,
                                "color": "#2196F3"
                              },
                              {
                                "type": "text",
                                "value": "Wind"
                              },
                              {
                                "type": "text",
                                "value": "{{currentWeather.wind}} km/h",
                                "style": {
                                  "fontWeight": "bold"
                                }
                              }
